 "css": ".print-format {\r\n    padding:0;\r\n}\r\n.print-format th {background-color: transparent !important;}\r\n.print-format td {padding: 0px !important;}\r\n@media print {\r\n\tbody {\r\n\tbackground: #fff;\r\n\tmargin: 0;\r\n\tpadding: 0;\r\n\tfont-family: Arial, sans-serif;\r\n\tfont-size: 0.45cm;\r\n\tcolor: #000;\r\n\t}\r\n}\r\n\r\n@media screen,handheld,projection,tv {\r\n\tbody{\r\n\t\tmargin: 0px 0px 0px 0px;\r\n\t\tcolor : #000000;\r\n\t\tbackground-color: #ffffff;\r\n\t\tfont-family: Arial;\r\n\t\tfont-size: 12px;\r\n\t}\r\n\t\r\n\ttable {\r\n\t\tborder-color:       #000000 #000000 #000000 #000000;\r\n\t}\r\n\t\r\n\tform {\r\n\t    padding:            0;\r\n\t    margin:             0;\r\n\t    display:            inline;\r\n\t}\r\n\t\r\n\tth {\r\n\t    font-weight:        bold;\r\n\t    font-size:          12px;\r\n\t    color:              #000000;\r\n\t    border-color:       #000000 #000000 #000000 #000000;\r\n\t}\r\n\t\r\n\ttd {\r\n\t\t\tfont-size:          16px;\r\n\t\t\tcolor:              #000000;\r\n\t\t\tvertical-align:     top;\r\n\t}\r\n}",
