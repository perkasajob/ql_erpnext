 "css": ".print-format {\n    padding:0 !important;\n    margin: 0px 0px 0px 0px !important;\n    width: 813px !important;\n\theight: 1058px !important;\n\tfont-size: 10pt !important;\n}\n.print-format th {background-color: transparent !important;}\n.print-format td {padding: 0px !important;}\n@media print {\n    @page{\n        size: 21.5cm 28cm !important;\n    }\n\tbody {\n    width: 813px !important;\n\theight: 1058px !important;\n\tbackground: #fff;\n\tmargin: 0 !important;\n\tpadding: 0 !important;\n\tfont-size: 10pt !important;\n\tcolor: #000;\n\t}\n}\n\n@media screen,handheld,projection,tv {\n\tbody{\n\t\tmargin: 0px 0px 0px 0px;\n\t\tcolor : #000000;\n\t\tbackground-color: #ffffff;\n\t\tfont-family: Arial;\n\t\tfont-size: 12px;\n\t}\n\t\n\ttable {\n\t\tborder-color:       #000000 #000000 #000000 #000000;\n\t}\n\t\n\tform {\n\t    padding:            0;\n\t    margin:             0;\n\t    display:            inline;\n\t}\n\t\n\tth {\n\t    font-weight:        bold;\n\t    font-size:          12px;\n\t    color:              #000000;\n\t    border-color:       #000000 #000000 #000000 #000000;\n\t}\n\t\n\ttd {\n\t\t\tfont-size:          16px;\n\t\t\tcolor:              #000000;\n\t\t\tvertical-align:     top;\n\t}\n}",
