 "html": "{%- set emptyrow =  {} -%}\r\n\r\n{%- if emptyrow.update({'idx':\"\", 'qty':0, 'description':'', 'uom':'', 'stock_uom':'','rate':'','amount':''}) -%} {% endif %}\r\n\r\n{%- for itemx in doc.items|batch(17,emptyrow) -%}\r\n<table style=\"width:24.2cm; \" border=\"0\" cellspacing=\"0\" cellpadding=\"0\">\r\n   <tbody>\r\n      <tr>\r\n         <td colspan=\"2\" align=\"left\" style=\"height:1.6cm;\">&nbsp;</td>\r\n      </tr>\r\n      <tr>\r\n         <td colspan=\"2\" align=\"center\" style=\"height:1cm;\"><span style=\"font-weight:bold; font-size:18px; letter-spacing:2px;\">&nbsp;</span></td>\r\n      </tr>\r\n      <tr>\r\n         <td colspan=\"2\" style=\"padding-top:3mm;\">\r\n            <table style=\"width:24.2cm;\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\">\r\n               <tbody>\r\n                  <tr>\r\n                     <td align=\"left\" style=\"width:19.2cm; padding-left:1.5cm;\">{{ doc.po_no }}</td>\r\n                     <td align=\"left\" style=\"width:5cm; height:1cm;\">{{ doc.name }}</td>\r\n                  </tr>\r\n               </tbody>\r\n            </table>\r\n         </td>\r\n      </tr>\r\n      <tr>\r\n         <td colspan=\"2\">\r\n            <table border=\"0\" align=\"left\" cellpadding=\"0\" cellspacing=\"0\" style=\"width:24.2cm;\">\r\n               <tbody>\r\n                  <tr>\r\n                     <td align=\"left\" style=\"width:12.1cm; height:3.8cm;\">&nbsp;</td>\r\n                     <td align=\"left\" style=\"width:12.1cm;\" valign=\"top\">\r\n                        <table border=\"0\" align=\"left\" cellpadding=\"0\" cellspacing=\"0\" width=\"100%\">\r\n                           <tbody>\r\n                              <tr>\r\n                                 <td colspan=\"3\" align=\"left\" style=\"height:0.6cm;\">&nbsp;</td>\r\n                              </tr>\r\n                              <tr>\r\n                                 <td valign=\"top\" align=\"left\" style=\"width:2cm;\">&nbsp;</td>\r\n                                 <td valign=\"top\" align=\"center\" style=\"width:3mm;\">&nbsp;</td>\r\n                                 <td valign=\"top\" style=\"padding-left:1cm;\" align=\"left\">\r\n                                    <span style=\"font-weight:bold;\">\r\n                                       <div style=\"white-space:normal;\">{{ doc.customer }}</div>\r\n                                    </span>\r\n                                 </td>\r\n                              </tr>\r\n                              <tr>\r\n                                 <td valign=\"top\" style=\"height:1.5cm;\" align=\"left\">&nbsp;</td>\r\n                                 <td valign=\"top\" style=\"height:1.5cm;\" align=\"center\">&nbsp;</td>\r\n                                 <td valign=\"top\" style=\"height:1.5cm; padding-left:1cm;\" align=\"left\">\r\n                                    <div style=\"white-space:normal;\">{{ doc.address_display.split(\"Phone:\")[0] if doc.address_display != None else ''}}<br></div>\r\n                                 </td>\r\n                              </tr>\r\n                              <tr>\r\n                                 <td align=\"left\">&nbsp;</td>\r\n                                 <td align=\"center\">&nbsp;</td>\r\n                                 <td align=\"left\" style=\"padding-left:1cm;\">{{ frappe.db.get_value(\"Customer\", doc.customer, \"tax_id\") }}</td>\r\n                              </tr>\r\n                           </tbody>\r\n                        </table>\r\n                     </td>\r\n                  </tr>\r\n               </tbody>\r\n            </table>\r\n         </td>\r\n      </tr>\r\n      <tr>\r\n         <td align=\"left\" style=\"height:5mm; padding-left:5cm !important;\">{{ doc.get_formatted('delivery_date') }}</td>\r\n         <td align=\"right\" style=\"height:5mm; padding-right:1cm;\"></td>\r\n      </tr>\r\n      <tr>\r\n         <td colspan=\"2\" align=\"left\">\r\n            <div style=\"height:11.7cm; overflow:hidden; white-space:nowrap; text-overflow:ellipsis\">\r\n               <table id=\"tab_items\" style=\"width:24.2cm;\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\">\r\n                  <tbody>\r\n                     <tr align=\"center\">\r\n                        <th style=\"width:1.5cm; height:1.3cm;\">&nbsp;</th>\r\n                        <th style=\"width:7cm;\">&nbsp;</th>\r\n                        <th style=\"width:4cm;\">&nbsp;</th>\r\n                        <th style=\"width:4cm;\">&nbsp;</th>\r\n                        <th style=\"width:4cm;\">&nbsp;</th>\r\n                     </tr> \r\n                     {%- for row in itemx -%}\r\n                        {%- if row.qty -%}\r\n                     <tr>\r\n                        <td valign=\"top\" align=\"right\" style=\"padding-right:8mm;\">{{ row.idx }}.&nbsp;</td>\r\n                        <td valign=\"top\" align=\"left\">\r\n                           <div style=\"width:7cm; white-space:no-wrap; overflow:hidden;\">{{ row.description }}</div>\r\n                        </td>\r\n                        <td valign=\"top\" align=\"right\" style=\"padding-right:2mm;\">{{ row.qty }} {{ row.uom or row.stock_uom }}</td>\r\n                        <td valign=\"top\" align=\"right\">{{ row.get_formatted(\"rate\", doc) }}</td>\r\n                        <td valign=\"top\" align=\"right\">{{ row.get_formatted(\"amount\", doc) }}</td>\r\n                     </tr>\r\n                     {% else %}\r\n                     <tr>\r\n                       <td style=\"width: 3%;\">&nbsp;-</td>\r\n                     </tr>\r\n                     {%- endif -%}\r\n                     {%- endfor -%}\r\n                  </tbody>\r\n               </table>\r\n            </div>\r\n         </td>\r\n      </tr>\r\n{%- if not loop.last -%}      \r\n      <tr>\r\n         <td colspan=\"2\" align=\"left\">\r\n            <table style=\"width:24.2cm;\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\">\r\n               <tbody>\r\n                  <tr>\r\n                     <td style=\"width:20.2cm;\" align=\"left\">&nbsp;</td>\r\n                     <td align=\"right\" style=\"width:4cm; height:0.85cm;\">&nbsp;</td>\r\n                  </tr>\r\n                  <tr>\r\n                     <td align=\"left\">&nbsp;</td>\r\n                     <td align=\"right\" style=\"height:0.85cm;\">&nbsp;</td>\r\n                  </tr>\r\n                  <tr>\r\n                     <td align=\"left\">&nbsp;</td>\r\n                     <td align=\"right\" style=\"height:0.83cm;\">&nbsp;</td>\r\n                  </tr>\r\n                  <tr>\r\n                     <td align=\"left\">&nbsp;</td>\r\n                     <td align=\"right\" style=\"height:0.85cm;\">&nbsp;</td>\r\n                  </tr>\r\n                  <tr>\r\n                     <td align=\"left\">&nbsp;</td>\r\n                     <td align=\"right\" style=\"height:0.82cm;\">&nbsp;</td>\r\n                  </tr>\r\n                  <tr>\r\n                     <td align=\"left\">&nbsp;</td>\r\n                     <td align=\"right\" style=\"height:0.83cm;\">&nbsp;</td>\r\n                  </tr>\r\n               </tbody>\r\n            </table>\r\n         </td>\r\n      </tr>\r\n      <tr>\r\n         <td colspan=\"2\" align=\"left\" style=\"padding-left:2cm; padding-top:2mm; height:1.5cm;\">&nbsp;</td>\r\n      </tr>\r\n      <tr>\r\n         <td align=\"left\" valign=\"top\" style=\"padding-left:11cm !important; padding-top:2mm;\">&nbsp;</td>\r\n         <td align=\"left\" valign=\"top\">\r\n            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{{ frappe.utils.formatdate(doc.get('posting_date'),\"dd MMM yyyy\") }}\r\n            <div style=\"padding-top:4.5cm;\">El Dan Alpha Jehosua</div>\r\n         </td>\r\n      </tr>\r\n   </tbody>\r\n</table>\r\n<br style=\" page-break-before: always;\">      \r\n{%- else -%}\r\n   <tr>\r\n      <td colspan=\"2\" align=\"left\">\r\n         <table style=\"width:24.2cm;\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\">\r\n            <tbody>\r\n               <tr>\r\n                  <td style=\"width:20.2cm;\" align=\"left\">&nbsp;</td>\r\n                  <td align=\"right\" style=\"width:4cm; height:0.85cm;\">{{ frappe.utils.fmt_money(doc.total, currency=\"Rp.\") }} </td>\r\n               </tr>\r\n               <tr>\r\n                  <td align=\"left\">&nbsp;</td>\r\n                  <td align=\"right\" style=\"height:0.85cm;\">{{ frappe.utils.fmt_money(doc.discount, currency=\" Rp.\") if doc.discount else '0' }}</td>\r\n               </tr>\r\n               <tr>\r\n                  <td align=\"left\">&nbsp;</td>\r\n                  <td align=\"right\" style=\"height:0.83cm;\">{{ frappe.utils.fmt_money((doc.discount_margin/100*(doc.total - doc.discount)) if doc.discount else '', currency=\" Rp.\") }}</td>\r\n               </tr>\r\n               <tr>\r\n                  <td align=\"left\">&nbsp;</td>\r\n                  <td align=\"right\" style=\"height:0.83cm;\">{{ frappe.utils.fmt_money(doc.net_total, currency=\" Rp.\") }}</td>\r\n               </tr>\r\n               <tr>\r\n                  <td align=\"left\">&nbsp;</td>\r\n                  <td align=\"right\" style=\"height:0.85cm;\">{{ frappe.utils.fmt_money(doc.taxes[0].tax_amount|round, currency=\"Rp.\") }}</td>\r\n               </tr>\r\n               <tr>\r\n                  <td align=\"left\">&nbsp;</td>\r\n                  <td align=\"right\" style=\"height:0.82cm;\">{{ frappe.utils.fmt_money(doc.taxes[1].tax_amount|round if doc.taxes|count > 1 else \"0\", currency=\"Rp.\") }}</td>\r\n               </tr>\r\n               <tr>\r\n                  <td align=\"left\">&nbsp;</td>\r\n                  <td align=\"right\" style=\"height:0.83cm;\">{{ frappe.utils.fmt_money(doc.rounded_total, currency=\"Rp.\") }}</td>\r\n               </tr>\r\n            </tbody>\r\n         </table>\r\n      </td>\r\n   </tr>\r\n   <tr>\r\n      <td colspan=\"2\" align=\"left\" style=\"padding-left:2cm; padding-top:2mm; height:1.5cm;\" valign=\"top\">{{ doc.in_words }}</td>\r\n   </tr>\r\n   <tr>\r\n      <td align=\"left\" valign=\"top\" style=\"padding-left:3.2cm !important; padding-top:2mm;\">&nbsp;&nbsp;&nbsp;&nbsp;{{ frappe.utils.formatdate(doc.get('due_date'),\"dd MMM yyyy\") }}</td>\r\n      <td align=\"left\" valign=\"top\" style=\"padding-left:12cm !important;\">\r\n         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{{ frappe.utils.formatdate(doc.get('posting_date'),\"dd MMM yyyy\") }}\r\n         <div style=\"padding-top:4.5cm;\">El Dan Alpha Jehosua</div>\r\n      </td>\r\n   </tr>\r\n   </tbody>\r\n</table>   \r\n{%- endif -%}\r\n{%- endfor -%}",
